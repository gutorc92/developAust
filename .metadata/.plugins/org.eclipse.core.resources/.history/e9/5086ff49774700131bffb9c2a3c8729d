package models;

import java.util.List;

public class Question {
	
	private int id;
	
	private String text;
	
	private List<Option> options;

	public Question() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Question(int id, String text, List<Option> options) {
		super();
		this.id = id;
		this.text = text;
		this.options = options;
	}
	
	
}
